{
  "name": "emailgenerator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "ngtw build",
    "start": "ng serve & ngtw watch",
    "build": "ngtw build && ng build",
    "build-prod": "ngtw build && ng build --prod",
    "prerender-build-release": "npm run prerender -- -c release --routesFile routes.txt",
    "prerender-build-prod": "npm run prerender -- -c production --routesFile routes.txt",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run emailGenerator:serve-ssr",
    "serve:ssr": "node dist/emailGenerator/server/main.js",
    "build:ssr": "ng build --prod && ng run emailgenerator:server:production",
    "prerender": "ng run emailGenerator:prerender",
    "serve-mobile": "ng serve --host=0.0.0.0 --disable-host-check & ngtw watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/platform-server": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@nguniversal/express-engine": "^9.0.2",
    "@ngx-lite/json-ld": "^0.6.2",
    "@trademe/ng-defer-load": "^8.0.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@nguniversal/builders": "^9.0.2",
    "@types/express": "^4.17.0",
    "@types/google.analytics": "0.0.39",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "tailwindcss": "^1.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
